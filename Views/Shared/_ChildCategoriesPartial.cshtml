@model IEnumerable<Rozetka.Data.Entity.Childcategory>

<div id="childcategories" class="col-md-4">
    @foreach (var child in Model)
    {
        <div class="child-category">
            <h5>@child.Name</h5>
            <ul>
                @foreach (var subChild in child.SubChildCategories)
                {
                    <li>@subChild.Name</li>
                }
            </ul>
        </div>
    }
</div>
